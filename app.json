{
  "name": "Swift Compiler Discord Bot",
  "keywords": [
    "swift",
    "compiler",
    "discord",
    "bot"
  ],
  "repository": "https://github.com/hoclos/SwiftDiscordBot",
  "stack": "container",
  "env": {
    "DISCORD_TOKEN": {
      "description": "A secret key for verifying the integrity of signed cookies.",
      "required": true
    },
    "NICKNAME": {
      "description": "Bot's nickname",
      "required": false
    },
    "ISSEASON": {
      "description": "Switch season mode",
      "required": false
    }
  }
}
